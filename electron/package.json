{
	"name": "referencer-electron",
	"version": "1.0.0",
	"description": "Referencer App built with Electron",
	"main": "api/main.ts",
	"scripts": {
		"dev:electron": "export $(cat config/dev.env | xargs) && webpack --watch --config-name api",
		"build:electron": "export $(cat config/prod.env | xargs) && webpack --config-name api",
		"start-inspect:electron": "export $(cat config/dev.env | xargs) && electron --inspect=5000 ./dist/electron/main.js",
		"start:electron": "electron ./dist/electron/main.js",
		"dev:react": "export $(cat config/dev.env | xargs) && webpack --watch --config-name ui",
		"build:react": "export $(cat config/prod.env | xargs) && webpack --config-name ui",
		"start:react": "webpack serve ./dist/renderer"
	},
	"author": "pj1301",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.13.16",
		"@babel/plugin-transform-runtime": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/runtime": "^7.13.17",
		"@types/node": "^15.0.1",
		"@types/react": "^17.0.4",
		"@types/react-dom": "^17.0.3",
		"babel-loader": "^8.2.2",
		"css-loader": "^5.2.4",
		"electron": "^12.0.5",
		"html-webpack-plugin": "^5.3.1",
		"node-sass": "^5.0.0",
		"nodemon": "^2.0.7",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^9.1.1",
		"typescript": "^4.2.4",
		"webpack": "^5.36.1",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.2"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
