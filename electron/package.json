{
	"name": "referencer-electron",
	"version": "1.0.0",
	"description": "Referencer App built with Electron",
	"main": "api/main.ts",
	"scripts": {
        "start": "NODE_ENV=development nodemon --exec ",
		"build": "tsc",
		"dev:electron": "NODE_ENV=development webpack --config webpack/electron.config.js --mode development && npm run start:electron",
		"dev:react": "NODE_ENV=development webpack-dev-server --config webpack/react.config.js --mode development",
		"start:electron": "electron .",
		"build:electron": "webpack --config webpack/electron.config.js",
		"build:react": "webpack --config webpack/react.config.js",
		"test": "jest"
	},
	"author": "pj1301",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.13.16",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@types/jest": "^26.0.23",
		"@types/react": "^16.14.5",
		"@types/react-dom": "^16.9.12",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"babel-loader": "^8.2.2",
		"electron": "^12.0.5",
		"electron-builder": "^22.10.5",
		"electron-is-dev": "^2.0.0",
		"electron-rebuild": "^2.3.5",
		"electron-reload": "^1.5.0",
		"electron-window-state": "^5.0.3",
		"eslint": "^7.25.0",
		"html-webpack-plugin": "^4.5.2",
		"jest": "^26.6.3",
		"nodemon": "^2.0.7",
		"ts-node": "^9.1.1",
		"typescript": "^3.9.9",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.2"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
